{
  "name": "infrastructure_modeling_engine",
  "version": "1.0.0",
  "description": "Infrastructure Modeling Engine (IME) v1",
  "type": "commonjs",
  "main": "dist/core.js",
  "exports": {
    ".": "./dist/core.js",
    "./frontend": "./dist/frontend.js",
    "./compiler": "./dist/compiler/index.js",
    "./validation": "./dist/validation/index.js",
    "./export": "./dist/export/index.js"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "maintained node versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "vite",
    "start": "node dist/core.js",
    "test": "npm run test:all",
    "terraform:fixtures": "npm run build && node dist/tools/renderFixtureTerraform.js .tmp/terraform-fixture",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test:unit": "npm run build && node --test dist/tests/step1.spec.js dist/tests/step2.validation.spec.js",
    "test:integration": "npm run build && node --test dist/tests/step4.frontend.spec.js dist/tests/step5.export.spec.js dist/tests/step6.integration.spec.js dist/tests/step7.extensibility.spec.js",
    "test:ui": "npm run build && node --test dist/tests/step7.ui.spec.js",
    "test:snapshots": "npm run build && node --test dist/tests/step3.compiler.spec.js",
    "test:snapshot-drift": "npm run build && node dist/tools/checkSnapshotDrift.js",
    "test:terraform": "npm run terraform:fixtures && terraform -chdir=.tmp/terraform-fixture fmt -check -recursive && terraform -chdir=.tmp/terraform-fixture init -backend=false -input=false && terraform -chdir=.tmp/terraform-fixture validate",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:ui && npm run test:snapshots && npm run test:snapshot-drift && npm run test:terraform",
    "dev:engine": "tsc -w",
    "build:engine": "tsc",
    "build:ui": "vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Miotis2004/Infrastructure_Modeling_Engine.git"
  },
  "bugs": {
    "url": "https://github.com/Miotis2004/Infrastructure_Modeling_Engine/issues"
  },
  "homepage": "https://github.com/Miotis2004/Infrastructure_Modeling_Engine#readme",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.11",
    "@vitejs/plugin-react": "^4.3.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
